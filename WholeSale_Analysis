    "# K-Means Clustering"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 443,
   "id": "435bbf75",
   "metadata": {},
   "outputs": [],
   "source": [
    "Sum_of_squared_distances = []\n",
    "K = range(2,15)\n",
    "for k in K:\n",
    "    km = KMeans(n_clusters=k)\n",
    "    km = km.fit(standard_scaled_wholesale)\n",
    "    Sum_of_squared_distances.append(km.inertia_)"
   ]
  },
